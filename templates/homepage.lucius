.home-guide-img-wrapper img {
    object-fit: cover;
    max-width: 100%;
    height: 100%;
    opacity: 0.7;
    background-color: white;
}

.home-guide-img-wrapper img:hover {
    opacity: 1;
}

.home-guide-img-wrapper:not(:last-child) {
    border-right: #cccccc solid 1px;
}

.iframe-wrapper {
    position: relative;
    padding-top: 56.25%;
}

#youtube-latest, #twitch-player {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}